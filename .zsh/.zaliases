# alias
alias v="~/nvim-osx64/bin/nvim"
alias del-dsstore="find . -name '.DS_Store' -type f -ls -delete"
alias rm="rm -i"
alias gs="git st"
alias gg="git graph"
alias gbd="git branch --merged | grep -v '*' | xargs -I % git branch -d %"
alias gcp="git checkout . | git pull"
alias ls="exa"
alias lss="exa -abgHliS"
alias lst="exa -T"
alias dc="docker"
alias yapf="yapf -i --style="{indent_dictionary_value=True}""
alias isort="isort -sl"
eval "$(thefuck --alias)"
